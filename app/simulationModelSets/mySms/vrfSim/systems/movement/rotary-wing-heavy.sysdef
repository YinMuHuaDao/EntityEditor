   (meta-data 
      (system-name  "Cargo")
      (system-description  "General Cargo helicopter, such as the CH-53 Super Stallion.")
      (allowed-state-repository-types "rotary-wing-entity-param")
      (system-categories "movement")
      (parameter-data-list 
         (bool-parameter-data 
            (parameter-name  "use-fuel")
            (variable-type  "DtRwBoolean")
            (display-label  "Enable original fuel model while using encumbered flight")
            (display-units  "True or False")
            (source-units  "True or False")
            (list-items  "")
            (list-display  0)
            (allow-edit  False)
            (default-value  False)
         )
         (real-parameter-data 
            (parameter-name  "fuel-amount")
            (variable-type  "DtRwReal")
            (display-label  "Fuel")
            (display-units  "liters")
            (source-units  "liters")
            (default-value  1420.000000)
         )
         (real-parameter-data 
            (parameter-name  "fuel-per-kN")
            (variable-type  "DtRwReal")
            (display-label  "Thrust Specific Fuel Consumption")
            (display-units  "fuel_units/s/kN")
            (source-units  "fuel_units/s/kN")
            (list-items  "")
            (list-display  0)
            (allow-edit  True)
            (roll-up-rule  "sum")
			   (decimal-places 10)
            (default-value  0.00000378)
         )
         (real-parameter-data 
