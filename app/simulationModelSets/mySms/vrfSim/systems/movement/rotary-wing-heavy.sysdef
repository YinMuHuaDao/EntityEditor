               (function-group $flight-controls)
               (description "Controls the yaw of this entity")
               (min-value -1.0)
               (max-value 1.0)
            )
            (collective
               (function-name "collective")
               (function-group $flight-controls)
               (description "Throttles the rotary wing")
               (min-value 0.0)
               (max-value 1.0)
            )
         )
